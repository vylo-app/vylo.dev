{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "@typescript-eslint",
      "prettier"
    ],
    "settings": {
      "import/resolver": {
        "alias": {
          "map": [["@", "./src"]],
         "extensions": [".ts", ".tsx", ".js", ".jsx"]
        }
      }
    },
    "rules": {
      "prettier/prettier": "error",
      "import/extensions": "off",
      // "eslint-plugin-import/no-unresolved": "off",
      "arrow-body-style": "off",
      "import/prefer-default-export": "off"
    }
  }
  
  